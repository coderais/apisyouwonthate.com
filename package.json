    "airtable": "^0.11.6",
    "axios": "^1.3.4",
    "date-fns": "^2.29.3",
    "fathom-client": "^3.5.0",
    "feed": "^4.2.2",
    "framer-motion": "^6.2.6",
    "gray-matter": "^4.0.3",
    "is-valid-email": "^0.0.5",
    "js-yaml": "^4.1.0",
    "jszip": "^3.10.1",
    "next": "^13.2.3",
    "next-mdx-remote": "4.2.0",
    "next-sitemap": "^4.0.2",
    "node-mailjet": "^6.0.2",
    "pluralize": "^8.0.0",
    "prism-react-renderer": "^1.3.5",
    "prismjs": "^1.29.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-helmet": "^6.1.0",
    "rehype-autolink-headings": "^6.1.1",
    "rehype-img-size": "^1.0.1",
    "rehype-slug": "^5.1.0",
    "sharp": "^0.31.3",
    "slugify": "^1.6.5",
    "string-strip-html": "^13.2.0",
    "turndown": "^7.1.1"
  },
  "devDependencies": {
    "@babel/core": "^7.21.3",
    "@babel/plugin-transform-runtime": "^7.21.0",
    "@babel/preset-env": "^7.20.2",
    "@mdx-js/loader": "^2.3.0",
    "@mdx-js/mdx": "^2.3.0",
    "babel-jest": "^29.5.0",
    "babel-plugin-transform-import-meta": "^2.2.0",
    "eslint": "8.35.0",
    "eslint-config-next": "13.2.3",
    "jest": "^29.5.0"
  },
  "keywords": [
    "apis"
  ],
  "license": "MIT",
  "scripts": {
    "dev": "next dev",
    "build": "node import-jobs.js && next build && next-sitemap",
    "postbuild": "next-sitemap",
    "lint": "next lint",
    "start": "next start",
    "test": "jest",
    "format": "prettier --write src/**/*.{js,jsx}",
    "prettier-check": "prettier \"src/./**/*.{js,json,css,md}\" --check",
    "prettier-fix": "prettier \"src/./**/*.{js,json,css,md}\" --write"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/apisyouwonthate/apisyouwonthate.com"
  },
  "bugs": {
    "url": "https://github.com/apisyouwonthate/apisyouwonthate.com/issues"
  },
  "jest": {
    "transform": {
      "\\.js$": [
        "babel-jest",
        {
          "configFile": "./babel-jest.config.js"
        }
      ],
      "\\.mjs$": [
        "babel-jest",
        {
          "configFile": "./babel-jest.config.js"
        }
      ],
      "^.+\\.jsx?$": "babel-jest",
      "^.+\\.mjs$": "babel-jest"
    },
    "testMatch": [
      "**/__tests__/**/*.(js|ts|tsx|mjs)",
      "**/?(*.)+(spec|test).(js|ts|tsx|mjs)"
    ]
  }
}
